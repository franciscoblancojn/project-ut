{
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "docker": "sudo docker build -t project-ut .",
        "-v": "php artisan --version",
        "db-restart": "php artisan migrate:reset && php artisan migrate && php artisan db:seed",
        "migrate": "php artisan make:migration && php artisan migrate",
        "restart": "php artisan route:clear && php artisan config:clear && php artisan cache:clear && php artisan view:clear",
        "serve": "npm run restart && php artisan serve",
        "start": "concurrently \"npm run serve\" \"npm run dev\"",
        "sass-min": "sass ./styles/index.scss:./styles/index.css --style compressed",
        "sass": "sass ./styles/index.scss:./styles/index.css",
        "lint": "npx eslint resources/src",
        "lint:fix": "npm run lint -- --fix",
        "prettier": "npx prettier resources/src '!**/*.stories.tsx' --check",
        "prettier:fix": "npm run prettier -- --write",
        "format": "npm run prettier:fix && npm run lint:fix",
        "flb": "npm run format && npm run lint && npm run build && npm run sass-min",
        "storybook": "storybook dev -p 6006",
        "update-lock": "npm i",
        "update-i": "bun i || pnpm i || npm i",
        "remove-bun-lock": "rm -r bun.lockb || true",
        "remove-pnpm-lock": "rm -rf pnpm-lock.yaml || true",
        "remove-npm-lock": "rm -r package-lock.json || true",
        "remove-lock": "npm run remove-bun-lock && npm run remove-pnpm-lock && npm run remove-npm-lock",
        "remove-node-module": "rm -r node_modules || true",
        "remove": "npm run remove-node-module && npm run remove-lock",
        "update-package": "ncu -u '/^fenextjs.*$/' '/^xell-ui-.*$/' '/^@xell-shop.*$/'",
        "update": "npm run update-package && npm run remove && npm run update-i && npm run update-lock && npm run flb",
        "u": "npm run update && git add . && git commit -m update && git push"
    },
    "dependencies": {
        "@inertiajs/react": "^2.0.3",
        "@tanstack/react-query": "5.59.16",
        "@vitejs/plugin-basic-ssl": "^1.2.0",
        "@vitejs/plugin-react": "^4.3.4",
        "fenextjs": "15.3.3",
        "path": "^0.12.7",
        "preact-router": "^4.1.2",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-router-dom": "^7.1.5",
        "vite-plugin-mkcert": "^1.17.6"
    },
    "devDependencies": {
        "@types/node": "^22.13.0",
        "@types/react": "^19.0.8",
        "@types/react-dom": "^19.0.3",
        "autoprefixer": "^10.4.20",
        "axios": "^1.7.4",
        "concurrently": "^9.1.2",
        "laravel-vite-plugin": "^1.2.0",
        "postcss": "^8.4.47",
        "tailwindcss": "^3.4.13",
        "typescript": "^5.7.3",
        "vite": "^6.0.11",
        "@typescript-eslint/eslint-plugin": "7.5.0",
        "@typescript-eslint/parser": "7.5.0",
        "eslint": "8.57.0",
        "eslint-plugin-storybook": "0.8.0",
        "npm-check-updates": "16.14.18",
        "prettier": "3.2.5",
        "ts-loader": "9.5.1",
        "tslib": "2.6.2",
        "eslint-config-next": "^14.0.1",
        "eslint-config-prettier": "9.0.0",
        "eslint-plugin-cypress": "^2.15.1",
        "eslint-plugin-import": "2.29.0",
        "eslint-plugin-jsx-a11y": "6.7.1",
        "eslint-plugin-react": "7.33.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "@storybook/addon-actions": "8.0.9",
        "@storybook/addon-essentials": "8.0.9",
        "@storybook/addon-interactions": "8.0.9",
        "@storybook/addon-links": "8.0.9",
        "@storybook/addon-onboarding": "8.0.9",
        "@storybook/blocks": "8.0.9",
        "@storybook/builder-webpack5": "8.0.9",
        "@storybook/manager-webpack5": "6.5.16",
        "@storybook/nextjs": "8.0.9",
        "@storybook/react": "8.0.9",
        "@storybook/testing-library": "0.2.2",
        "sass": "^1.82.0",
        "storybook": "8.0.9",
        "storybook-dark-mode": "4.0.1"
    }
}